
var namesList=["raj","rahul","rajdeep","rajan","rupam"];
var flag=0;
var promptInput=prompt("Please enter your name");
for(var i=0;i<namesList.length;i++){
    if(namesList[i]==promptInput){
        flag=1;
        break;
    }
}
if(flag==1){
    alert("Welcome back "+promptInput);    
}else{
    alert("You are not registered with us. Please register with us")
}

