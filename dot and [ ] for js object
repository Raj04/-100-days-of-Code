function phoneticLookup(val) {
  var result = "";

  // Only change code below this line
  var lookup={
    "alpha": "Adams",
    "bravo": "Boston",
    "charlie": "Chicago",
    "delta": "Denver",
    "echo": "Easy",
    "foxtrot": "Frank",
  }
  result=lookup[val];  // doesn't work for lookup.val
  return result;
}

// Change this value to test
phoneticLookup("charlie");
